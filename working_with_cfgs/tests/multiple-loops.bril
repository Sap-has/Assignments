@main {
  a: int = const 0;
  b: int = const 0;
.loop1:
  a: int = add a 1;
  br a .loop1 .between;
.between:
  c: int = const 1;
.loop2:
  b: int = add b 1;
  br b .loop2 .end;
.end:
  ret;
}

